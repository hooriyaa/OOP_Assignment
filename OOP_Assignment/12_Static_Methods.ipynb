    "# 12. Static Methods - TemperatureConverter\n",
    "\n",
    "class TemperatureConverter:\n",
    "    @staticmethod\n",
    "    def celsius_to_fahrenheit(c):\n",
    "        return (c * 9/5) + 32\n",
    "\n",
    "# Test\n",
    "print(\"20째C is\", TemperatureConverter.celsius_to_fahrenheit(20), \"째F\")"
   ]
