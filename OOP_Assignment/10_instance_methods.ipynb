    "# 10. Instance Methods - Dog Class\n",
    "class Dog:\n",
    "    def __init__(self, name, breed):\n",
    "        self.name = name\n",
    "        self.breed = breed\n",
    "    \n",
    "    def bark(self):\n",
    "        print(f\"{self.name} the {self.breed} says Woof!\")\n",
    "\n",
    "# Test\n",
    "dog = Dog(\"Buddy\", \"Golden Retriever\")\n",
    "dog.bark()"
   ]
  }
