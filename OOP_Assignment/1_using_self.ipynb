    "### 1. Using self - Student Class:\n",
    "\n",
    "class Student:\n",
    "    def __init__(self, name, marks):\n",
    "        self.name = name\n",
    "        self.marks = marks\n",
    "    \n",
    "    def display(self):\n",
    "        print(f\"Student: {self.name}, Marks: {self.marks}\")\n",
    "\n",
    "# Test\n",
    "student = Student(\"Alice\", 95)\n",
    "student.display()\n"
   ]
